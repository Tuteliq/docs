{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "Tuteliq",
  "description": "Documentation for the Tuteliq API â€” AI-powered child safety detection for text, voice, images, and video.",
  "colors": {
    "primary": "#2DD4BF",
    "light": "#5EEAD4",
    "dark": "#14B8A6"
  },
  "favicon": "/favicon.svg",
  "icons": {
    "library": "fontawesome"
  },
  "styling": {
    "eyebrows": "breadcrumbs",
    "codeblocks": "dark"
  },
  "search": {
    "prompt": "Search Tuteliq docs..."
  },
  "feedback": {
    "thumbsRating": true,
    "suggestEdit": true,
    "raiseIssue": true
  },
  "api": {
    "baseUrl": "https://api.tuteliq.ai",
    "auth": {
      "method": "bearer"
    },
    "playground": {
      "display": "interactive"
    },
    "examples": {
      "languages": ["bash", "javascript", "python"]
    }
  },
  "openapi": "https://api.tuteliq.ai/docs/json",
  "navigation": {
    "tabs": [
      {
        "tab": "Guides",
        "groups": [
          {
            "group": "Getting Started",
            "icon": "play",
            "pages": [
              "index",
              "quickstart",
              "authentication",
              "how-it-works",
              "credits",
              "changelog"
            ]
          },
          {
            "group": "SDKs",
            "icon": "puzzle-piece",
            "pages": [
              "sdks/node",
              "sdks/python",
              "sdks/swift",
              "sdks/kotlin",
              "sdks/flutter",
              "sdks/react-native",
              "sdks/dotnet",
              "sdks/unity",
              "sdks/cli",
              "sdks/mcp"
            ]
          },
          {
            "group": "Advanced",
            "icon": "bolt",
            "pages": [
              "languages",
              "voice-streaming",
              "video-analysis",
              "webhooks",
              "error-handling"
            ]
          },
          {
            "group": "Compliance",
            "icon": "scale-balanced",
            "pages": [
              "kosa-compliance",
              "gdpr",
              "trust",
              "certification",
              "sdlc"
            ]
          }
        ]
      },
      {
        "tab": "API Reference",
        "groups": [
          {
            "group": "Overview",
            "pages": [
              "api-reference/introduction"
            ]
          }
        ]
      }
    ],
    "global": {
      "anchors": [
        {
          "anchor": "API Status",
          "href": "https://tuteliq.ai/status",
          "icon": "signal"
        },
        {
          "anchor": "Dashboard",
          "href": "https://tuteliq.ai/dashboard",
          "icon": "gauge"
        }
      ]
    }
  },
  "logo": {
    "light": "/logo/light.svg",
    "dark": "/logo/dark.svg"
  },
  "navbar": {
    "links": [
      {
        "label": "Support",
        "href": "mailto:support@tuteliq.ai"
      }
    ],
    "primary": {
      "type": "button",
      "label": "Get API Key",
      "href": "https://tuteliq.ai/dashboard"
    }
  },
  "contextual": {
    "options": [
      "copy",
      "view",
      "chatgpt",
      "claude",
      "cursor",
      "vscode"
    ]
  },
  "footer": {
    "socials": {
      "x": "https://x.com/tuteliq",
      "github": "https://github.com/Tuteliq",
      "linkedin": "https://linkedin.com/company/tuteliq"
    }
  },
  "seo": {
    "indexHiddenPages": false
  },
  "metadata": {
    "og:site_name": "Tuteliq Docs",
    "og:image": "https://tuteliq.ai/og-image.png",
    "twitter:site": "@tuteliq",
    "robots": "index, follow",
    "author": "Tuteliq"
  },
  "redirects": [
    { "source": "/docs", "destination": "/index" },
    { "source": "/api", "destination": "/api-reference/introduction" }
  ]
}
