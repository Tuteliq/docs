{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "Tuteliq",
  "colors": {
    "primary": "#6366F1",
    "light": "#818CF8",
    "dark": "#4F46E5"
  },
  "favicon": "/favicon.svg",
  "api": {
    "baseUrl": "https://api.tuteliq.ai",
    "auth": {
      "method": "bearer"
    }
  },
  "openapi": "https://api.tuteliq.ai/docs/json",
  "navigation": {
    "tabs": [
      {
        "tab": "Guides",
        "groups": [
          {
            "group": "Getting Started",
            "pages": [
              "index",
              "quickstart",
              "authentication",
              "credits"
            ]
          },
          {
            "group": "SDKs",
            "pages": [
              "sdks/node",
              "sdks/python",
              "sdks/swift"
            ]
          },
          {
            "group": "Advanced",
            "pages": [
              "voice-streaming",
              "webhooks"
            ]
          },
          {
            "group": "Compliance",
            "pages": [
              "kosa-compliance",
              "gdpr"
            ]
          }
        ]
      },
      {
        "tab": "API Reference",
        "groups": [
          {
            "group": "Overview",
            "pages": [
              "api-reference/introduction"
            ]
          }
        ]
      }
    ],
    "global": {
      "anchors": [
        {
          "anchor": "API Status",
          "href": "https://tuteliq.ai/status",
          "icon": "signal"
        },
        {
          "anchor": "Dashboard",
          "href": "https://tuteliq.ai/dashboard",
          "icon": "gauge"
        }
      ]
    }
  },
  "logo": {
    "light": "/logo/light.svg",
    "dark": "/logo/dark.svg"
  },
  "navbar": {
    "links": [
      {
        "label": "Support",
        "href": "mailto:support@tuteliq.ai"
      }
    ],
    "primary": {
      "type": "button",
      "label": "Get API Key",
      "href": "https://tuteliq.ai/dashboard"
    }
  },
  "contextual": {
    "options": [
      "copy",
      "view",
      "chatgpt",
      "claude",
      "cursor",
      "vscode"
    ]
  },
  "footer": {
    "socials": {
      "x": "https://x.com/tuteliq",
      "github": "https://github.com/Tuteliq",
      "linkedin": "https://linkedin.com/company/tuteliq"
    }
  },
  "seo": {
    "indexHiddenPages": false
  },
  "metadata": {
    "og:site_name": "Tuteliq Docs",
    "og:image": "https://tuteliq.ai/og-image.png",
    "twitter:site": "@tuteliq"
  }
}
